    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Port 3000",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port=3000",
            ]
        },
        {
            "name": "Debug WebServer 3000",
            "request": "launch",
            "type": "dart",
            "deviceId": "web-server",
            "args": [
                "--web-renderer=html",
                "--web-port=3000",
                "--web-hostname=0.0.0.0"
            ]
        }
    ]
